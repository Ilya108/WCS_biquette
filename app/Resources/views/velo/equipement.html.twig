<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Equipement</title>
</head>
<body>
<h3>Equipement</h3>
{{ form_start(form) }}
{{ form_label(form.equipements) }}
<div id="appbundle_velo_equipement" class="">
    {% for equipement in equipements %}
        <label class="containerradio">
            <input type="checkbox" {% if equipement in velo.equipements  %}checked{% endif %} id="appbundle_VeloEquipement_equipement_{{ equipement.id }}" name="appbundle_VeloEquipement[equipement]" required="required" value="{{ equipement.id }}">
            <span class="checkmark"></span>
        </label>
    {% endfor %}
</div>
    {% do form.equipements.setRendered %}
<div class="butSave"><input type="submit" value="Enregistrer" /></div>
{{ form_end(form) }}
</body>
</html>